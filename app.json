{
  "expo": {
    "name": "jmu-food-bank",
    "slug": "jmu-food-bank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/food-pantry-ios-icon.png",
    "splash": {
      "image": "./assets/food-pantry-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/food-pantry-adaptive-icon.png",
        "backgroundColor": "#310171"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "View the pop up pantry inventory and reserve items for pickup"
  }
